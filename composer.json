{
    "name": "artworx/omegacp",
    "description": "A Laravel ragnarok online admin panel",
    "keywords": ["laravel", "ragnarok online", "ragnarok", "admin", "panel", "omegacp"],
    "license": "MIT",
    "homepage": "https://github.com/carloquilala/omegacp",
    "support": {
        "issues": "https://github.com/carloquilala/omegacp/issues",
        "source": "https://github.com/carloquilala/omegacp"
    },
    "authors": [
        {
            "name": "Jupeto",
            "email": "jupeto24@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.3.0|~5.4.0",
        "intervention/image": "^2.3",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.0",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.0.35"
    },
    "require-dev": {
        "phpunit/phpcov": "~3.0",
        "phpunit/phpunit": "~5.0",
        "laravel/framework": "~5.3.8|5.4.*",
        "orchestra/testbench": "3.3.*|3.4.x-dev",
        "orchestra/database": "3.3.*|3.4.x-dev",
        "laravel/browser-kit-testing": "~1.0.0",
        "orchestra/testbench-browser-kit": "~3.3.0|~3.4.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
